{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:4000"
      }
    }
  },
  "Api": {
    "Enabled": true,
    "ListenAddress": "0.0.0.0",
    "Port": 4000,
    "EnableMetrics": true,
    "MetricsIpRestricted": false,
    "MetricsIpWhitelist": [],
    "RateLimiting": {
      "Disabled": true
    }
  }
}
